<plugin>
	<nom>
		<multi>
		[fr]Une Licence pour mes articles et mes documents
		[en]A license for articles and documents
		</multi>
	</nom>
	<icon>img_pack/licence_logo.png</icon>
	<auteur>
		<multi>
		[fr]fanouch - lesguppies@free.fr
    	<br />Sur une idée originale de :  <a href='http://www.spip-contrib.net/Filtre-Licence'>erational</a>
		[en]fanouch - lesguppies@free.fr
		<br />Based on an original idea of : <a href='http://www.spip-contrib.net/Filtre-Licence'>erational</a>
		</multi>
	</auteur>
	<version>0.2.1</version>
	<version_base>0.2</version_base>
	<etat>test</etat>
	<description>
		<multi>
			[fr]Permet de relier une licence d'utilisation à un article ou un document
			[en]Link a license on articles and documents
		</multi>
	</description>
	<lien>http://www.spip-contrib.net/spip.php?article2147</lien>
	<fonctions>licence_fonctions.php</fonctions>
	<prefix>licence</prefix>
	<install>base/licence_upgrade.php</install>
	<pipeline>
		<nom>declarer_tables_principales</nom>
		<inclure>base/licence.php</inclure>
	</pipeline>
	<pipeline>
		<nom>affiche_milieu</nom>
		<inclure>licence_pipelines.php</inclure>
	</pipeline>
	<pipeline>
		<nom>pre_insertion</nom>
		<inclure>licence_pipelines.php</inclure>
	</pipeline>
	<pipeline>
		<nom>pre_edition</nom>
		<inclure>licence_pipelines.php</inclure>
	</pipeline>
	<pipeline>
		<nom>editer_contenu_objet</nom>
		<inclure>licence_pipelines.php</inclure>
	</pipeline>
	<necessite id="SPIP" version="[2.0.10;]" />
	<necessite id="spip_bonux" version="[1.8.7;]" />
</plugin>
