	<nom>Administration des mots-clés</nom>
	<auteur>cpaulus@quesaco.org</auteur>
	<version>0.2002</version>
	<etat>test</etat>
	<icon>images/amocles.png</icon>
	<description>Permet de :<ul><li>déléguer l'administration des mots-clés</li><li>importer et exporter vos mots-clés via un fichier texte.</li></ul></description>
	<lien>[Documentation->http://www.quesaco.org/Administrer-les-mots-cles]</lien>
	
	<prefix>amocles</prefix>

	<options>amocles_mes_options.php</options>
	
	<install>base/amocles_install.php</install>

	<pipeline>
		<nom>ajouter_boutons</nom>
		<action>ajouter_boutons</action>
		<inclure>inc/amocles_pipeline_ajouter_boutons.php</inclure>
	</pipeline>

	<pipeline>
		<nom>ajouter_onglets</nom>
		<inclure>inc/amocles_pipeline_ajouter_onglets.php</inclure>
	</pipeline>

	<chemin dir='' />
</plugin>
