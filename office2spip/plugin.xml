<plugin>
	<nom>Office2SPIP</nom>
	<auteur>ARNO*</auteur>
	<version>0.7</version>
	<etat>dev</etat>
	<description>
		Office2SPIP fabrique des articles SPIP directement à partir de fichiers Office (Word, OpenOffice...). 
		
		Nécessite Unoconv sur le serveur.
	</description>
	<icon>logo/logo.png</icon>
	<lien></lien> <!-- URI de documentation -->
	<licence>GPL 3</licence>
	<prefix>office2spip</prefix>
	<pipeline>
		<nom>affiche_enfants</nom>
		<inclure>office2spip.php</inclure>
	</pipeline>

	<necessite id="SPIP" version="[2.0;]" />

</plugin>

