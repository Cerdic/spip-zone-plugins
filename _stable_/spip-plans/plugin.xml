<?xml version="1.0" encoding="utf-8"?>
<plugin>
	<nom>SPIP-Plans</nom>
	<auteur>[Artégo->http://www.artego.fr]</auteur>
	<version>2.0</version>
	<etat>stable</etat>
	<licence>[GPLv3->http://www.gnu.org/licenses/gpl-3.0.html]</licence>
	<description>Création de plans interactifs</description>
	<necessite id="SPIP" version="[2.0.3;]" />
	<necessite id="surcharges" version="[2.0;]" />
	<install>base/plans.php</install>
	<lien>[Site officiel du plugin->http://www.artego.fr/-Plans-interactifs-]</lien>
	<icon>prive/images/plan-48.png</icon>
	<options>plans_options.php</options>
	<fonctions>plans_fonctions.php</fonctions>
	<prefix>plans</prefix>
	<pipeline>
		<nom>declarer_tables_interfaces</nom>
		<inclure>base/plans.php</inclure>
	</pipeline>
	<pipeline>
		<nom>declarer_tables_principales</nom>
		<inclure>base/plans.php</inclure>
	</pipeline>
	<pipeline>
		<nom>declarer_tables_auxiliaires</nom>
		<inclure>base/plans.php</inclure>
	</pipeline>
	<pipeline>
		<nom>autoriser</nom>
		<inclure>inc/plans_autorisations.php</inclure>
	</pipeline>
	<pipeline>
		<nom>declarer_tables_objets_surnoms</nom>
		<inclure>inc/plans_pipelines.php</inclure>
	</pipeline>
	<pipeline>
		<nom>rechercher_liste_des_champs</nom>
		<inclure>inc/plans_pipelines.php</inclure>
	</pipeline>
	<pipeline>
		<nom>header_prive</nom>
		<inclure>inc/plans_pipelines.php</inclure>
	</pipeline>
	<pipeline>
		<nom>insert_head</nom>
		<inclure>inc/plans_pipelines.php</inclure>
	</pipeline>
	<pipeline>
		<nom>editer_contenu_objet</nom>
		<inclure>inc/plans_pipelines.php</inclure>
	</pipeline>
	<pipeline>
		<nom>libelle_association_mots</nom>
		<inclure>inc/plans_pipelines.php</inclure>
	</pipeline>
	<bouton id="plans_tous" parent="naviguer">
		<icone>prive/images/plan-24.png</icone>
		<titre>plans:plans</titre>       
	</bouton>
</plugin>