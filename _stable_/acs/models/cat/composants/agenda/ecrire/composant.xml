<composant>
  <!-- Nom du composant -->
  <nom>Agenda</nom>
  <!-- Auteur du composant -->
  <auteur>Daniel FAIVRE &copy; 2008-2009 - licence GPL</auteur>
  <version>0.4.1</version>
	<necessite id='acs' version='[0.4.1;]' />

  <param>
    <nom>preview</nom>
    <valeur>160</valeur>
  </param>

  <variable>
    <nom>TitreFondColor</nom>
    <type>color</type>
    <valeur>acsRubnavFond</valeur>
  </variable>
  <variable>
    <nom>TitreFondImage</nom>
    <type>img</type>
    <chemin>img/trame</chemin>
    <label>non</label>
  </variable>
  <variable>
    <nom>HeadFondColor</nom>
    <type>color</type>
    <valeur>acsRubnavFond</valeur>
  </variable>
  <variable>
    <nom>BordColor</nom>
    <type>color</type>
    <valeur>acsRubnavBord</valeur>
  </variable>
  <variable>
    <nom>BordLargeur</nom>
    <type>largeurbord</type>
    <valeur>thin</valeur>
    <label>non</label>
  </variable>
  <variable>
    <nom>BordStyle</nom>
    <type>stylebord</type>
    <valeur>solid</valeur>
    <label>non</label>
  </variable>
  <variable>
    <nom>NotThisMonth</nom>
    <type>color</type>
    <valeur>acsRubnavFondHover</valeur>
  </variable>
  <variable>
    <nom>ThisMonth</nom>
    <type>color</type>
    <valeur>acsFondColor</valeur>
  </variable>
  <variable>
    <nom>ThisDay</nom>
    <type>color</type>
    <valeur>acsFondColor</valeur>
  </variable>
  <variable>
    <nom>BulleFondColor</nom>
    <type>color</type>
    <valeur>acsFondColor</valeur>
  </variable>
  <variable>
    <nom>BulleVoirArticlesModifies</nom>
    <type>choix</type>
    <option>oui</option>
    <option>non</option>
    <valeur>non</valeur>
  </variable>
  <variable>
    <nom>BulleArticleModifieFondColor</nom>
    <type>color</type>
    <valeur>acsFondColor</valeur>
  </variable>
  <variable>
    <nom>BulleBreveFondColor</nom>
    <type>color</type>
    <valeur>acsFondColor</valeur>
  </variable>
  <variable>
    <nom>BulleSurvol</nom>
    <type>color</type>
    <valeur>acsRubnavFondHover</valeur>
  </variable>
</composant>