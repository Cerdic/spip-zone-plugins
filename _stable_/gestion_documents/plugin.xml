	</pipeline>
	<pipeline>
		<nom>post_edition</nom>
		<inclure>gestdoc_pipelines.php</inclure>
	</pipeline>
	<bouton id='documents' parent='naviguer'>
		<titre>gestdoc:documents</titre>
		<icone>img_pack/portfolio-24.png</icone>
		<url>portfolio</url>
