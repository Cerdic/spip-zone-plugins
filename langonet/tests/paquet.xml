<!-- Dans ce fichier :
	- 3 cas d'utilisation X1 repérés
		- 1 item défini dans langotests
		- 1 item non défini dans langotests
		- 1 item défini mais dans public
	- les autres identifiants ne sont pas repérés car pas sous la forme module:raccourci
-->
<paquet
	prefix="langotests"
	categorie="outil"
	version="0.1.0"
	etat="dev"
	compatibilite="[3.0.0;3.0.*]"
	logo="langotests-64.png"
>	

	<nom>Base de tests de LangOnet</nom>

	<auteur lien="http://blog.smellup.net">Eric Lupinacci</auteur>
	<auteur>denisb</auteur>

	<licence lien="http://www.gnu.org/licenses/gpl-3.0.html">GPL 3</licence>

	<pipeline nom="declarer_tables_objets_sql" inclure="base/langotests_declarations.php" />
	<pipeline nom="declarer_tables_interfaces" inclure="base/langotests_declarations.php" />

	<!-- X1 / module : langotests / item : defini_2 / repéré / défini  -->
	<onglet nom="onglet_1" titre="langotests:defini_2" parent="langonet" action="action_1" />
	<!-- X1 / module : langotests / item : indefini_2 / repéré / indéfini  -->
	<onglet nom="onglet_2" titre="langotests:item_indefini_2" parent="langonet" action="action_2" />
	<!-- X1 / module : vide (public) / item : voir_en_ligne / repéré / défini  -->
	<menu nom="menu" titre="voir_en_ligne" parent="bando_administration" icone="langotests-16.png" action="action_3" />
</paquet>
