<plugin>
	<nom>jQuery Corner</nom>
	<slogan>Le plugin qui arrondit les angles</slogan>
	<auteur>Yffic</auteur>
	<licence>GPL v3</licence>
	<version>0.4.2</version>
	<version_base>0.1</version_base>
	<etat>test</etat>
	<description>
		<multi>
			[fr]Une adaptation pour Spip du plugin [JQuery Corner->http://jquery.malsup.com/corner/]
		</multi>
	</description>
	<icon>prive/themes/spip/images/jquerycorner-32.png</icon>
	<lien>http://contrib.spip.net/JQuery-Corner</lien>
	<prefix>jquerycorner</prefix>
	<install>base/jquerycorner_install.php</install>
	<pipeline>
		<nom>insert_head</nom>
		<inclure>jquerycorner_pipelines.php</inclure>
	</pipeline>
	<pipeline>
		 <nom>autoriser</nom>
		 <inclure>inc/jquerycorner_autoriser.php</inclure>
	</pipeline>
	<pipeline>
		<nom>ieconfig_metas</nom>
		<inclure>jquerycorner_ieconfig_metas.php</inclure>
	</pipeline>
	<bouton id='jquerycorner' parent='configuration'>
		<icone>prive/themes/spip/images/jquerycorner-24.png</icone>
		<titre>jquerycorner:titre_menu</titre>
		<url>configurer_jquerycorner</url>
	</bouton>
	<bouton id='jquerycorner2' parent='bando_squelette'>
		<icone>prive/themes/spip/images/jquerycorner-16.png</icone>
		<titre>jquerycorner:titre_menu</titre>
		<url>configurer_jquerycorner</url>
	</bouton>
	<necessite id="SPIP" version="[2.1.0;3.0.99]" />
	<necessite id="saisies" version="[1.9.0;]" />
	<necessite id="spip_bonux" version="[1.8;]" />
	<necessite id="YAML" version="[1.5;]" /> <!-- pour Saisies -->
	<categorie>multimedia</categorie>
</plugin>