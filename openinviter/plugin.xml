<plugin>
	<nom>OpenInviter</nom>
	<auteur>[Les Développements Durables->http://www.ldd.fr]</auteur>
	<licence>GPL v3</licence>
	<version>1.1.0</version>
	<etat>stable</etat>
	<slogan>Intégration de la librairie OpenInviter.</slogan>
	<description>Intégration de la librairie OpenInviter avec l'API du plugin Importateur de contacts.</description>
	<categorie>outil</categorie>
	<icon>images/openinviter.gif</icon>
	<traduire gestionnaire="salvatore" module="openinviter" reference="fr" />
	<lien>http://www.spip-contrib.net/OpenInviter</lien>
	
	<prefix>openinviter</prefix>
	<options>openinviter_options.php</options>
	
	<pipeline>
		<nom>importateur_contacts_moteurs</nom>
		<inclure>openinviter_pipelines.php</inclure>
	</pipeline>
	
	<necessite id="SPIP" version="[2.1.10;3.0.99]" />
	<utilise id="importateur_contacts" />
</plugin>
