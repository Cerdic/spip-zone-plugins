<composant>
  <!-- Nom du composant -->
  <nom>Video</nom>

  <!-- Auteur du composant -->
  <auteur>Daniel FAIVRE &copy; 2008-2011 - licence GPL</auteur>
  <version>0.8.1</version>
  <necessite id='acs' version='[0.7.1;]' />

  <param>
    <nom>preview</nom>
    <valeur>250</valeur>
  </param>
  <param>
    <nom>instanciable</nom>
  </param>
  
  <variable>
    <nom>Fond</nom>
    <type>color</type>
    <valeur>#000</valeur>
  </variable>
  <variable>
    <nom>Icons</nom>
    <type>color</type>
    <valeur>#009966</valeur>
  </variable>
  <variable>
    <nom>IconsHover</nom>
    <type>color</type>
    <valeur>#00ff00</valeur>
  </variable>
  <variable>
    <nom>Titre</nom>
    <type>color</type>
    <valeur>#dfdfdf</valeur>
  </variable>
  <variable>
    <nom>Width</nom>
    <type>text</type>
    <valeur>320</valeur>
  </variable>
  <variable>
    <nom>Height</nom>
    <type>text</type>
    <valeur>200</valeur>
  </variable>

  <variable>
    <nom>Bord</nom>
    <type>bord</type>
  </variable>
  
  <variable>
    <nom>Shadow</nom>
    <type>color</type>
  </variable>
  <variable>
    <nom>ShadowSize</nom>
    <type>text</type>
  </variable>
  <variable>
    <nom>ShadowBlur</nom>
    <type>text</type>
  </variable>

  <variable>
    <nom>Download</nom>
    <type>choix</type>
    <option>oui</option>
    <option>non</option>
    <valeur>oui</valeur>
  </variable>
</composant>