<composant>
  <!-- Nom du composant -->
  <nom>Agenda</nom>
  <!-- Auteur du composant -->
  <auteur>Daniel FAIVRE &copy; 2008-2010 - licence GPL</auteur>
  <version>0.7.2</version>
	<necessite id='acs' version='[0.7.1;]' />

  <param>
    <nom>preview</nom>
    <valeur>180</valeur>
  </param>

  <variable>
    <nom>TitreFond</nom>
    <type>color</type>
  </variable>
  <variable>
    <nom>TitreFondImage</nom>
    <type>img</type>
    <label>non</label>
  </variable>
  <variable>
    <nom>HeadFondColor</nom>
    <type>color</type>
    <valeur>=acsRubnavFond</valeur>
  </variable>
  <variable>
    <nom>Bord</nom>
    <type>bord</type>
  </variable>
  <variable>
    <nom>NotThisMonth</nom>
    <type>color</type>
    <valeur>=acsRubnavFondHover</valeur>
  </variable>
  <variable>
    <nom>ThisMonth</nom>
    <type>color</type>
    <valeur>=acsFondColor</valeur>
  </variable>
  <variable>
    <nom>ThisDay</nom>
    <type>color</type>
    <valeur>=acsFondColor</valeur>
  </variable>
  <variable>
    <nom>BulleFondColor</nom>
    <type>color</type>
    <valeur>=acsFondColor</valeur>
  </variable>
  <variable>
    <nom>BulleVoirArticlesModifies</nom>
    <type>choix</type>
    <option>oui</option>
    <option>non</option>
    <valeur>non</valeur>
  </variable>
  <variable>
    <nom>BulleArticleModifieFondColor</nom>
    <type>color</type>
    <valeur>=acsFondColor</valeur>
  </variable>
  <variable>
    <nom>BulleBreveFondColor</nom>
    <type>color</type>
    <valeur>=acsFondColor</valeur>
  </variable>
  <variable>
    <nom>BulleSurvol</nom>
    <type>color</type>
    <valeur>=acsRubnavFondHover</valeur>
  </variable>

  <variable>
    <nom>Shadow</nom>
    <type>color</type>
  </variable>
  <variable>
    <nom>ShadowSize</nom>
    <type>nombre</type>
  </variable>
  <variable>
  	<nom>ShadowBlur</nom>
    <type>nombre</type>
  </variable>
</composant>