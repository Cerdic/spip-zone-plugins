<composant>
  <!-- Nom du composant -->
  <nom>Fond</nom>
  <!-- Auteur du composant -->
  <auteur>Daniel FAIVRE &copy; 2008-2009 - licence GPL</auteur>
  <version>0.5</version>
  <necessite id='acs' version='[0.5;]' />

  <param>
    <nom>preview</nom>
    <valeur>150</valeur>
  </param>
  <param>
    <nom>optionnel</nom>
    <valeur>non</valeur>
  </param>
  
	<variable>
    <nom>Favicon</nom>
    <type>img</type>
		<chemin>img/favicon</chemin>
  </variable>
  <variable>
    <nom>Color</nom>
    <type>color</type>
    <valeur>#ffffff</valeur>
  </variable>
  <variable>
    <nom>Text</nom>
    <type>color</type>
    <valeur>#000000</valeur>
  </variable>
  <variable>
    <nom>Link</nom>
    <type>color</type>
    <valeur>#00008d</valeur>
  </variable>
  <variable>
    <nom>LinkHover</nom>
    <type>color</type>
    <valeur>#0000ff</valeur>
  </variable>
  <variable>
    <nom>Image</nom>
    <type>img</type>
    <chemin>img/fond</chemin>
    <label>non</label>
  </variable>

  <variable>
    <nom>FontSize</nom>
    <type>nombre</type>
		<valeur>1em</valeur>
  </variable>
  <variable>
    <nom>FontFamily</nom>
    <type>fontFamily</type>
		<valeur>sans-serif</valeur>
  </variable>
  <variable>
    <nom>Font</nom>
    <type>text</type>
		<valeur>Verdana, 'Trebuchet MS', Arial, 'Lucida Sans'</valeur>
  </variable>

  <variable>
    <nom>DeplierHaut</nom>
    <type>img</type>
    <chemin>img/triangles</chemin>
    <valeur>deplierhaut.gif</valeur>
  </variable>
  <variable>
    <nom>DeplierHaut_rtl</nom>
    <type>img</type>
    <chemin>img/triangles</chemin>
    <valeur>deplierhaut_rtl.gif</valeur>
  </variable>
  <variable>
    <nom>DeplierBas</nom>
    <type>img</type>
    <chemin>img/triangles</chemin>
    <valeur>deplierbas.gif</valeur>
  </variable>
</composant>