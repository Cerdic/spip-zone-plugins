<composant>
  <!-- Nom du composant -->
  <nom>Navgroup</nom>
  <!-- Auteur du composant -->
  <auteur>Daniel FAIVRE &copy; 2008-2011 - licence GPL</auteur>
  <version>0.8.1</version>
  <necessite id='acs' version='[0.7.2;]' />
  
  <param>
    <nom>preview</nom>
    <valeur>160</valeur>
  </param>
  <param>
    <nom>instanciable</nom>
  </param>

  <variable>
    <nom>Nom</nom>
    <type>text</type>
  </variable>

  <variable>
    <nom>Style</nom>
    <type>choix</type>
    <option>accordion</option>
    <option>menu</option>
    <valeur>accordion</valeur>
  </variable>
  
  <variable>
    <nom>Titre</nom>
    <type>choix</type>
    <option>oui</option>
    <option>non</option>
    <valeur>oui</valeur>
  </variable>
  <variable>
    <nom>TitreFond</nom>
    <type>color</type>
  </variable>
  <variable>
    <nom>TitreFondImage</nom>
    <type>img</type>
    <chemin>trame</chemin>
    <label>non</label>
  </variable>
  <variable>
    <nom>Fond</nom>
    <type>color</type>
  </variable>  
  <variable>
    <nom>Bord</nom>
    <type>bord</type>
  </variable>
  <variable>
    <nom>BordRond</nom>
    <type>text</type>
  </variable>
  <variable>
    <nom>Sep</nom>
    <type>color</type>
  </variable>
  <variable>
    <nom>Bottom</nom>
    <type>text</type>
  </variable>

  <variable>
    <nom>Shadow</nom>
    <type>color</type>
  </variable>
  <variable>
    <nom>ShadowSize</nom>
    <type>text</type>
  </variable>
  <variable>
    <nom>ShadowBlur</nom>
    <type>text</type>
  </variable>
</composant>