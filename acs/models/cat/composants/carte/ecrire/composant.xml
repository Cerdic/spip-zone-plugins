<composant>
  <!-- Nom du composant -->
  <nom>Carte</nom>
  <!-- Auteur du composant -->
  <auteur>Daniel FAIVRE &copy; 2008-2011 - licence GPL</auteur>
  <version>0.8.3</version>
  <necessite id='acs' version='[0.7.1;]' />
  <necessite id='gis' version='[0.2;]' />
  <necessite id='openlayers' version='[0.0.2;]' />

  <param>
    <nom>preview</nom>
    <valeur>300</valeur>
  </param>
  <param>
    <nom>preview_type</nom>
    <valeur>inline</valeur>
  </param>
  <param>
    <nom>instanciable</nom>
  </param>

  <variable>
    <nom>Nom</nom>
    <type>text</type>
  </variable>
  <variable>
    <nom>Lat</nom>
    <type>text</type>
  </variable>
  <variable>
    <nom>Lon</nom>
    <type>text</type>
  </variable>
  <variable>
    <nom>Zoom</nom>
    <type>text</type>
  </variable>

  <variable>
    <nom>ShowLayerSwitcher</nom>
    <type>choix</type>
    <option>oui</option>
    <option>non</option>
    <valeur>oui</valeur>
  </variable>
  <variable>
    <nom>OpenStreetMap</nom>
    <type>choix</type>
    <option>oui</option>
    <option>non</option>
    <valeur>oui</valeur>
  </variable>
  <variable>
    <nom>Osmarender</nom>
    <type>choix</type>
    <option>oui</option>
    <option>non</option>
    <valeur>oui</valeur>
  </variable>
  <variable>
    <nom>WMS</nom>
    <type>choix</type>
    <option>oui</option>
    <option>non</option>
    <valeur>oui</valeur>
  </variable>
  
  <variable>
    <nom>gPhy</nom>
    <type>choix</type>
    <option>oui</option>
    <option>non</option>
    <valeur>oui</valeur>
  </variable>
  <variable>
    <nom>gMap</nom>
    <type>choix</type>
    <option>oui</option>
    <option>non</option>
    <valeur>oui</valeur>
  </variable>
  <variable>
    <nom>gHyb</nom>
    <type>choix</type>
    <option>oui</option>
    <option>non</option>
    <valeur>oui</valeur>
  </variable>
  <variable>
    <nom>gSat</nom>
    <type>choix</type>
    <option>oui</option>
    <option>non</option>
    <valeur>oui</valeur>
  </variable>

  <variable>
    <nom>Picto</nom>
    <type>img</type>
    <chemin>pictos</chemin>
    <valeur>interrogation.png</valeur>
  </variable>
  <variable>
    <nom>PictoHover</nom>
    <type>img</type>
    <chemin>pictos</chemin>
    <valeur>interrogation_over.png</valeur>
  </variable>
  <variable>
    <nom>PictosPins</nom>
    <type>choix</type>
    <option>oui</option>
    <option>non</option>
    <valeur>non</valeur>
  </variable>
  <variable>
    <nom>PictosArticles</nom>
    <type>KeyGroup</type>
    <label>non</label>
  </variable>
  <variable>
    <nom>PictosRubriques</nom>
    <type>KeyGroup</type>
    <label>non</label>
  </variable>
  <variable>
    <nom>Mots</nom>
    <type>choix</type>
    <option>oui</option>
    <option>non</option>
    <valeur>oui</valeur>
    <label>non</label>
  </variable>
  <variable>
    <nom>Rubriques</nom>
    <type>choix</type>
    <option>oui</option>
    <option>non</option>
    <valeur>oui</valeur>
    <label>non</label>
  </variable>
  <variable>
    <nom>Articles</nom>
    <type>choix</type>
    <option>oui</option>
    <option>non</option>
    <valeur>oui</valeur>
    <label>non</label>
  </variable>
    
  <variable>
    <nom>Close</nom>
    <type>img</type>
    <chemin>mapui</chemin>
    <valeur>close.gif</valeur>
  </variable>
  <variable>
    <nom>CloseOver</nom>
    <type>img</type>
    <chemin>mapui</chemin>
    <valeur>close_over.gif</valeur>
  </variable>
  
  <variable>
    <nom>TitreFontSize</nom>
    <type>text</type>
    <valeur>1.09em</valeur>
  </variable>
  <variable>
    <nom>TitreFontFamily</nom>
    <type>fontFamily</type>
    <valeur>sans-serif</valeur>
  </variable>
  <variable>
    <nom>TitreFont</nom>
    <type>text</type>
  </variable>
  
  <variable>
    <nom>TextFontSize</nom>
    <type>text</type>
    <valeur>0.88em</valeur>
  </variable>
  <variable>
    <nom>TextFontFamily</nom>
    <type>fontFamily</type>
    <valeur>sans-serif</valeur>
  </variable>
  <variable>
    <nom>TextFont</nom>
    <type>text</type>
  </variable>

  <variable>
    <nom>ListRubriques</nom>
    <type>choix</type>
    <option>oui</option>
    <option>non</option>
    <valeur>oui</valeur>
  </variable>
  <variable>
    <nom>ListArticles</nom>
    <type>choix</type>
    <option>oui</option>
    <option>non</option>
    <valeur>oui</valeur>
  </variable>
  <variable>
    <nom>LastUpdate</nom>
    <type>choix</type>
    <option>oui</option>
    <option>non</option>
    <valeur>oui</valeur>
  </variable>
  <variable>
    <nom>LogoMax</nom>
    <type>text</type>
    <value>100</value>
  </variable>
  
</composant>