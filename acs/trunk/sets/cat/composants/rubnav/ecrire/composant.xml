<composant>
  <!-- Nom du composant -->
  <nom>Rubnav</nom>
  <!-- Auteur du composant -->
  <auteur>Daniel FAIVRE &copy; 2008-2015 - licence GPL</auteur>
  <version>1.0.0</version>
  <necessite id='acs' version='[0.7.1;]' />
  <group>nav</group>

  <param>
    <nom>optionnel</nom>
    <valeur>no</valeur>
  </param>
  <param>
    <nom>preview</nom>
    <valeur>200</valeur>
  </param>

  <variable>
    <nom>Fond</nom>
    <type>color</type>
  </variable>
  <variable>
    <nom>Fond2</nom>
    <type>color</type>
  </variable>
  <variable>
    <nom>Fond3</nom>
    <type>color</type>
  </variable>
  <variable>
    <nom>Fond4</nom>
    <type>color</type>
  </variable>
  <variable>
    <nom>Fond5</nom>
    <type>color</type>
  </variable>
  <variable>
    <nom>Fond6</nom>
    <type>color</type>
  </variable>
  <variable>
    <nom>FondHover</nom>
    <type>color</type>
  </variable>
  <variable>
    <nom>Titre</nom>
    <type>choix</type>
    <option>oui</option>
    <option>non</option>
    <valeur>oui</valeur>
  </variable>
  <variable>
    <nom>TitreFond</nom>
    <type>color</type>
  </variable>
  <variable>
    <nom>TitreFondImage</nom>
    <type>img</type>
    <chemin>trame</chemin>
    <label>non</label>
  </variable>
  <variable>
    <nom>Bord</nom>
    <type>bord</type>
  </variable>
  <variable>
    <nom>BordRond</nom>
    <type>text</type>
  </variable>
  <variable>
    <nom>Sep</nom>
    <type>color</type>
  </variable>

  <variable>
    <nom>Shadow</nom>
    <type>color</type>
  </variable>
  <variable>
    <nom>ShadowSize</nom>
    <type>text</type>
  </variable>
  <variable>
  	<nom>ShadowBlur</nom>
    <type>text</type>
  </variable>
  
  <variable>
    <nom>DeplierHaut</nom>
    <type>img</type>
    <chemin>triangles</chemin>
    <valeur>deplierhaut.gif</valeur>
  </variable>
  <variable>
    <nom>DeplierHautOver</nom>
    <type>img</type>
    <chemin>triangles</chemin>
    <valeur>deplierhauton.gif</valeur>
  </variable>
  <variable>
    <nom>DeplierHaut_rtl</nom>
    <type>img</type>
    <chemin>triangles</chemin>
    <valeur>deplierhaut_rtl.gif</valeur>
  </variable>
  <variable>
    <nom>DeplierHautOver_rtl</nom>
    <type>img</type>
    <chemin>triangles</chemin>
    <valeur>deplierhaut_rtl.gif</valeur>
  </variable>
  <variable>
    <nom>DeplierBas</nom>
    <type>img</type>
    <chemin>triangles</chemin>
    <valeur>deplierbas.gif</valeur>
  </variable>
  <variable>
    <nom>DeplierBasOver</nom>
    <type>img</type>
    <chemin>triangles</chemin>
    <valeur>deplierbas.gif</valeur>
  </variable>
</composant>