<composant>
  <!-- Nom du composant -->
  <nom>Navkeyrub</nom>
  <!-- Auteur du composant -->
  <auteur>Daniel FAIVRE &copy; 2008-2012 - licence GPL</auteur>
  <version>0.8.7</version>
  <necessite id='acs' version='[0.7.2;]' />
  <necessite id='spip' version='[1.9.2;]' />
  <group>nav</group>

  <param>
    <nom>preview</nom>
    <valeur>250</valeur>
  </param>
  <param>
    <nom>instanciable</nom>
  </param>

  <variable>
    <nom>Nom</nom>
    <type>text</type>
  </variable>

  <variable>
    <nom>Orientation</nom>
    <type>choix</type>
    <option>vertical</option>
    <option>horizontal</option>
    <valeur>vertical</valeur>
  </variable>

  <variable>
    <nom>Bord</nom>
    <type>bord</type>
  </variable>
  <variable>
    <nom>Bord2</nom>
    <type>bord</type>
  </variable>
  <variable>
    <nom>BordRond</nom>
    <type>text</type>
  </variable>
  <variable>
    <nom>Sep</nom>
    <type>color</type>
  </variable>

  <variable>
    <nom>Fond</nom>
    <type>color</type>
  </variable>
  <variable>
    <nom>Fond2</nom>
    <type>color</type>
  </variable>
  <variable>
    <nom>Fond3</nom>
    <type>color</type>
  </variable>
  <variable>
    <nom>Fond4</nom>
    <type>color</type>
  </variable>
  <variable>
    <nom>Fond5</nom>
    <type>color</type>
  </variable>
  <variable>
    <nom>Fond6</nom>
    <type>color</type>
  </variable>
  <variable>
    <nom>FondHover</nom>
    <type>color</type>
  </variable>

  <variable>
    <nom>Shadow</nom>
    <type>color</type>
  </variable>
  <variable>
    <nom>ShadowSize</nom>
    <type>text</type>
  </variable>
  <variable>
    <nom>ShadowBlur</nom>
    <type>text</type>
  </variable>

  <variable>
    <nom>GroupeMots</nom>
    <type>keygroup</type>
  </variable>

  <variable>
    <nom>Titre</nom>
    <type>choix</type>
    <option>oui</option>
    <option>non</option>
    <valeur>oui</valeur>
  </variable>
  <variable>
    <nom>TitreFond</nom>
    <type>color</type>
  </variable>
  <variable>
    <nom>TitreFondImage</nom>
    <type>img</type>
    <chemin>trame</chemin>
    <label>non</label>
  </variable>

  <variable>
    <nom>Affiche</nom>
    <type>choix</type>
    <option>logo</option>
    <option>titre</option>
    <option>les2</option>
    <valeur>les2</valeur>
  </variable>
  <variable>
    <nom>LogoMax</nom>
    <type>text</type>
    <valeur>20</valeur>
  </variable>
  <variable>
    <nom>CouleurMot</nom>
    <type>choix</type>
    <option>oui</option>
    <option>non</option>
    <valeur>oui</valeur>
  </variable>
  <variable>
    <nom>Articles</nom>
    <type>choix</type>
    <option>oui</option>
    <option>non</option>
    <valeur>non</valeur>
  </variable>
  <variable>
    <nom>FondArticle</nom>
    <type>color</type>
  </variable>
  <variable>
    <nom>Sub</nom>
    <type>choix</type>
    <option>toujours</option>
    <option>survol</option>
    <valeur>toujours</valeur>
  </variable>
  <variable>
    <nom>Timer</nom>
    <type>text</type>
    <valeur>400</valeur>
  </variable>
</composant>