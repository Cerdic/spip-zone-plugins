<plugin>
	<nom>Multilang</nom>
	<auteur>Renato Formato</auteur>
	<licence>GNU/GPL</licence>
	<version>0.0.3</version>
	<version_base>0.1</version_base>
	<etat>dev</etat>
	<description>
		<multi>
			[fr]Gestion des tags multi
			[it]Gestione facile dei tag multi
			[en]Easy management of the multilingual tags
		</multi>
	</description>
	<prefix>multilang</prefix>
	<icon>images/multilang-32.png</icon>
	<install>base/multilang_install.php</install>
	<necessite id="SPIP" version="[2.1;]" />
	<necessite id="saisies" version="[1.1;]" />
	<necessite id="spip_bonux" version="[1.8;]" />
 	<necessite id="cfg" version="[1.12;]" />
	<pipeline>
		<nom>header_prive</nom>
		<action>insert_head_prive</action>
		<inclure>multilang_pipelines.php</inclure>
	</pipeline>
	<pipeline>
		<nom>insert_head_css</nom>
		<inclure>multilang_pipelines.php</inclure>
	</pipeline>
	<pipeline>
		<nom>insert_head</nom>
		<inclure>multilang_pipelines.php</inclure>
	</pipeline>
	<categorie>edition</categorie>
</plugin>
