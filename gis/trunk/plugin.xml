<plugin>
	<nom>GIS</nom>
	<auteur>b_b, kent1</auteur>
	<version>2.0.4</version>
	<version_base>2.0.1</version_base>
	<licence>GNU/GPL</licence>
	<etat>dev</etat>
	<description>
