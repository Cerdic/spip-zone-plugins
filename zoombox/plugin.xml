	<prefix>Zoombox</prefix>
	<pipeline>
		<nom>insert_head_css</nom>
		<inclure>head.php</inclure>
	</pipeline>
	<pipeline>
		<nom>insert_head</nom>
		<inclure>head.php</inclure>
	</pipeline>
	<pipeline>
	 <nom>insert_js</nom>
	 <inclure>head.php</inclure>
	</pipeline>
