<plugin>
	<nom> <!-- Nom du plugin -->
	Kay&eacute;
	</nom>
	<auteur>	<!-- Auteur du plugin -->
	C&eacute;dric Couvrat
	<br>
	Inspection Acad&eacute;mique de la Vienne
	</auteur>
	<version>
	0.2
	</version>
	<etat>
	dev
	</etat>
	<description>
	Cahier de texte &eacute;lectronique spip pour l'&eacute;cole primaire
	</description>
	<fonctions>kaye_mes_fonctions.php</fonctions>
	<prefix> 
	kaye
	</prefix>
	<!-- Definitions des fonctions qui s'inserent dans les pipelines -->
	<pipeline>
		<nom>ajouter_boutons</nom>
		<action>ajouterBoutons</action>
		<inclure>inc_kaye.php</inclure>
	</pipeline>
	<!-- Le parametre action est facultatif : en son absence la methode de la classe portant le nom du pipeline est appelee. Le parametre inclure est facultatif.	-->
	<pipeline>
		<nom>header_prive</nom>
		<action>header_prive</action>
		<inclure>kaye_mes_fonctions.php</inclure>
	</pipeline>
	<pipeline>
		<nom>insert_head</nom>
		<action>insert_head</action>
		<inclure>inc_kaye.php</inclure>
	</pipeline>
</plugin>
