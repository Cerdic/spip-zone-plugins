<composant>
  <!-- Nom du composant -->
  <nom>Liste_articles</nom>
  <!-- Auteur du composant -->
  <auteur>Daniel FAIVRE &copy; 2008 - licence GPL</auteur>
  <version>0.2</version>
  <version_spip_min>1.9207</version_spip_min>
  <version_spip_max>1.9208</version_spip_max>

  <param>
    <nom>preview</nom>
    <valeur>250</valeur>
  </param>

  <variable>
    <nom>TitresFondColor</nom>
    <type>color</type>
    <valeur>acsOngletsFondColor</valeur>
  </variable>
  <variable>
    <nom>TitresFondImage</nom>
    <type>img</type>
    <chemin>img/trame</chemin>
    <label>non</label>
  </variable>
  <variable>
    <nom>BordColor</nom>
    <type>color</type>
    <valeur>acsOngletsBordColor</valeur>
  </variable>
  <variable>
    <nom>BordLargeur</nom>
    <type>largeurbord</type>
    <valeur>acsFondBordLargeur</valeur>
    <label>non</label>
  </variable>
  <variable>
    <nom>BordStyle</nom>
    <type>stylebord</type>
    <label>non</label>
    <valeur>dotted</valeur>
  </variable>
  <variable>
    <nom>MargeBas</nom>
    <type>nombre</type>
    <valeur>5</valeur>
  </variable>
  <variable>
    <nom>RubFond2</nom>
    <type>color</type>
    <valeur>fafffa</valeur>
  </variable>
  <variable>
    <nom>RubFond3</nom>
    <type>color</type>
    <valeur>f5fff5</valeur>
  </variable>
  <variable>
    <nom>RubFond4</nom>
    <type>color</type>
    <valeur>efffef</valeur>
  </variable>
  <variable>
    <nom>RubFond5</nom>
    <type>color</type>
    <valeur>dfffdf</valeur>
  </variable>
  <variable>
    <nom>RubFond6</nom>
    <type>color</type>
    <valeur>d0ffd0</valeur>
  </variable>
  <variable>
    <nom>Puce</nom>
    <type>img</type>
    <chemin>img/puces</chemin>
  </variable>
  <variable>
    <nom>Dates</nom>
    <type>choix</type>
    <option>oui</option>
    <option>non</option>
    <valeur>oui</valeur>
  </variable>
  <variable>
    <nom>Aut</nom>
    <type>choix</type>
    <option>oui</option>
    <option>non</option>
    <valeur>oui</valeur>
  </variable>
  <variable>
    <nom>NbLettres</nom>
    <type>nombre</type>
    <valeur>200</valeur>
  </variable>
  <variable>
    <nom>LogoTailleMax</nom>
    <type>nombre</type>
    <valeur>50</valeur>
  </variable>
</composant>