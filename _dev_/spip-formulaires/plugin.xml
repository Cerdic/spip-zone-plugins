<?xml version="1.0" encoding="utf-8"?>
<plugin>
	<nom>Plugin SPIP-Formulaires</nom>
	<auteur>[Artégo->http://www.artego.fr]</auteur>
	<version>2.0</version>
	<etat>stable</etat>
	<licence>[GPLv3->http://www.gnu.org/licenses/gpl-3.0.html]</licence>
	<description>Générateur de formulaires. Nécessite la bibliothèque PHP mcrypt.</description>
	<necessite id="SPIP" version="[2.0.3;]" />
	<necessite id="facteur" version="[1;]" />
	<necessite id="surcharges" version="[1.4;]" />
	<install>base/lettres.php</install>
	<lien>[Site officiel du plugin->http://www.artego.fr/-Generateur-de-formulaires-]</lien>
	<icon>prive/images/formulaire-48.png</icon>
	<options>formulaires_options.php</options>
	<fonctions>formulaires_fonctions.php</fonctions>
	<prefix>formulaires</prefix>
	<pipeline>
		<nom>declarer_tables_interfaces</nom>
		<inclure>base/formulaires.php</inclure>
	</pipeline>
	<pipeline>
		<nom>declarer_tables_principales</nom>
		<inclure>base/formulaires.php</inclure>
	</pipeline>
	<pipeline>
		<nom>declarer_tables_auxiliaires</nom>
		<inclure>base/formulaires.php</inclure>
	</pipeline>
	<pipeline>
		<nom>autoriser</nom>
		<inclure>inc/formulaires_autorisations.php</inclure>
	</pipeline>
	<pipeline>
		<nom>header_prive</nom>
		<inclure>inc/formulaires_pipelines.php</inclure>
	</pipeline>
	<pipeline>
		<nom>declarer_tables_objets_surnoms</nom>
		<inclure>inc/formulaires_pipelines.php</inclure>
	</pipeline>
	<pipeline>
		<nom>rechercher_liste_des_champs</nom>
		<inclure>inc/formulaires_pipelines.php</inclure>
	</pipeline>
	<pipeline>
		<nom>tester_rubrique_vide</nom>
		<inclure>inc/formulaires_pipelines.php</inclure>
	</pipeline>
	<pipeline>
		<nom>calculer_rubriques</nom>
		<inclure>inc/formulaires_pipelines.php</inclure>
	</pipeline>
	<pipeline>
		<nom>trig_propager_les_secteurs</nom>
		<inclure>inc/formulaires_pipelines.php</inclure>
	</pipeline>
	<pipeline>
		<nom>calculer_langues_rubriques</nom>
		<inclure>inc/formulaires_pipelines.php</inclure>
	</pipeline>
	<pipeline>
		<nom>contenu_naviguer</nom>
		<inclure>inc/formulaires_pipelines.php</inclure>
	</pipeline>
	<pipeline>
		<nom>editer_contenu_objet</nom>
		<inclure>inc/formulaires_pipelines.php</inclure>
	</pipeline>
	<pipeline>
		<nom>libelle_association_mots</nom>
		<inclure>inc/formulaires_pipelines.php</inclure>
	</pipeline>
	<bouton id="formulaires_tous" parent="naviguer">
		<icone>prive/images/formulaire-24.png</icone>
		<titre>formulairesprive:formulaires</titre>       
	</bouton>
	<onglet id="config_formulaires" parent="configuration">
		<icone>prive/images/formulaire-24.png</icone>
		<titre>formulairesprive:formulaires</titre>
	</onglet>
</plugin>